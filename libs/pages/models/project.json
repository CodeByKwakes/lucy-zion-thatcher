{
  "name": "pages-models",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/pages/models/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/pages/models",
        "main": "libs/pages/models/src/index.ts",
        "tsConfig": "libs/pages/models/tsconfig.lib.json",
        "assets": ["libs/pages/models/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/pages/models/**/*.ts",
          "libs/pages/models/package.json"
        ]
      }
    }
  },
  "tags": []
}
